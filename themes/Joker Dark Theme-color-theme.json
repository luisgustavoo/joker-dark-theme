		"statusBar.background": "#141414",
		"statusBar.debuggingBackground": "#9D00FF",
		"button.background": "#9D00FF",
		"progressBar.background": "#9D00FF",
		"focusBorder": "#8b898970",
		"selection.background": "#8b898970",
		"list.activeSelectionBackground": "#8b898970",
		"pickerGroup.foreground": "#C576F6",
		"list.highlightForeground": "#C576F6",
		"badge.background": "#4FCC26",
		"list.dropBackground": "#8b898970",
		"editorWidget.background": "#141414",
		// "editorSuggestWidget.selectedForeground": "#9D00FF",
		"editor.findMatchBackground": "#264f78",
		"editor.findMatchHighlightBackground": "#8b898970",
		"editor.lineHighlightBackground": "#8b89892a",
		// "editor.lineHighlightBorder": "#8b89892a",
		// "editorLineNumber.activeForeground": "#ff0000"
		// "editorBracketHighlight.foreground1": "#ffff",
		// "editorBracketHighlight.foreground2": "#ffff",
		// "editorBracketHighlight.foreground3": "#ffff",
		// "editorBracketHighlight.foreground4": "#ffff",
		// "editorBracketHighlight.foreground5": "#ffff",
		// "editorBracketHighlight.foreground6": "#ffff",
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder",
			],
			"settings": {
				"foreground": "#ffff",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#D91A2A"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier",
			],
			"settings": {
				"foreground": "#5CFF5C",
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#5CFF5C",
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#C576F6"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#5CFF5C",
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				// "constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#5CFF5C"
			}
		},
		{
			"name": "Number, String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				// "constant.numeric",
				"string",
				"constant.other.symbol",
				"constant.other.key",
				// "entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#FFFF00"
			}
		},
		{
			"name": "Inherited Class",
			"scope": [
				"entity.other.inherited-class",
			],
			"settings": {
				"foreground": "#C576F6"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#C576F6",
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type",
				// "constant.numeric",
			],
			"settings": {
				"foreground": "#C576F6"
			}
		},
		{
			"name": "numeric",
			"scope": [
				// "support.type",
				"constant.numeric",
			],
			"settings": {
				"foreground": "#D91A2A",
				"fontStyle": "bold"
			}
		},
		{
